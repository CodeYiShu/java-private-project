<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.codeshu.dao.UserDao">
    <select id="list" resultType="com.codeshu.entity.User">
        SELECT id, name, age
        FROM user
    </select>

    <insert id="save" parameterType="com.codeshu.entity.User">
        INSERT INTO user(id, name, age)
        VALUES (#{id}, #{name}, #{age})
    </insert>
</mapper>

