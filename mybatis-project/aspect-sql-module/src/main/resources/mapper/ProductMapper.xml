<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.codeshu.mapper.ProductMapper">
    <select id="getProduct" resultType="com.codeshu.entity.ProductEntity">
        SELECT *
        FROM product WHERE name IS NOT NULL
    </select>
</mapper>
